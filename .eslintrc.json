{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",

        "linebreak-style": "off",

        "no-underscore-dangle": "off",

        "camelcase": "off"
    }
}
